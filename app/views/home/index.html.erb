			<div class="row">
				<input name="name" type="text" class="form-control contact_el" placeholder="Name" required>
			</div>
			<div class="row">
				<input name="email" type="email" class="form-control contact_el" placeholder="Email" required>
			</div>
			<div class="row">
				<input name="subject" type="text" class="form-control contact_el" placeholder="Subject" required>
			</div>
			<div class="row">
